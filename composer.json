{
    "name"             : "leandro/nfe",
    "description"      : "NFe para PHP por Leandro",
    "keywords"         : [
        "nfe", "php"
    ],
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Leandro Motta",
            "email": "leandrommoliveira@gmail.com"
        }
